{
  "name": "n8n-nodes-twenty-dynamic",
  "version": "2.0.0-beta.1",
  "description": "Advanced GraphQL n8n node for Twenty CRM with custom database, field support, and resource locators. Complete CRUD support for custom and default databases",
  "keywords": [
    "n8n-community-node-package",
    "twenty-crm",
    "crm",
    "graphql",
    "graphql-api",
    "typescript",
    "people",
    "companies",
    "opportunities",
    "notes",
    "sales-pipeline",
    "twenty-fork",
    "community-fork",
    "unified-search",
    "smart-fields",
    "custom-fields",
    "field-resolution",
    "automation",
    "workflow",
    "modern-architecture",
    "modular",
    "integration",
    "api",
    "dynamic-nodes",
    "n8n-nodes",
    "open-source-crm"
  ],
  "main": "index.js",
  "index": "dist/nodes/Twenty/Twenty.node.js",
  "license": "MIT",
  "homepage": "https://github.com/Logrui/n8n-nodes-twenty-dynamic",
  "author": {
    "name": "Logrui",
    "email": "yhcsanction@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Logrui/n8n-nodes-twenty-dynamic"
  },
  "bugs": {
    "url": "https://github.com/Logrui/n8n-nodes-twenty-dynamic/issues"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TwentyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Twenty/Twenty.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.17.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}