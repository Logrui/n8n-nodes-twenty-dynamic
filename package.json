{
  "name": "n8n-nodes-twenty-dynamic",
  "version": "0.10.0-beta.2",
  "description": "n8n node for Twenty CRM with dynamic GraphQL-based operations and attachment management",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Logrui/n8n-nodes-twenty-dynamic",
  "author": {
    "name": "Logrui",
    "email": "yhcsanction@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Logrui/n8n-nodes-twenty-dynamic"
  },
  "bugs": {
    "url": "https://github.com/Logrui/n8n-nodes-twenty-dynamic/issues"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TwentyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Twenty/Twenty.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
